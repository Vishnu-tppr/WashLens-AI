# Supabase Configuration
# Get these values from Supabase Dashboard -> Settings -> API
SUPABASE_URL=https://xxxxx.supabase.co
SUPABASE_ANON_KEY=eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9.your_key_here

# Firebase Configuration
# Get these values from Firebase Console -> Project Settings -> Your Apps

# Android
FIREBASE_ANDROID_API_KEY=your_android_api_key_here
FIREBASE_ANDROID_APP_ID=your_android_app_id_here
FIREBASE_PROJECT_ID=washlens-ai
FIREBASE_MESSAGING_SENDER_ID=your_sender_id_here
FIREBASE_STORAGE_BUCKET=washlens-ai.appspot.com

# iOS
FIREBASE_IOS_API_KEY=your_ios_api_key_here
FIREBASE_IOS_APP_ID=your_ios_app_id_here
FIREBASE_IOS_CLIENT_ID=your_ios_client_id_here
FIREBASE_IOS_BUNDLE_ID=com.washlens.ai

# Web (if needed)
FIREBASE_WEB_API_KEY=your_web_api_key_here
FIREBASE_WEB_APP_ID=your_web_app_id_here

# App Group (for iOS Widget)
IOS_APP_GROUP_ID=group.com.washlens.ai

# TFLite Model Configuration
TFLITE_MODEL_PATH=assets/models/washlens_yolo.tflite
TFLITE_LABELS_PATH=assets/models/labels.txt

# API Configuration (if using cloud inference fallback)
CLOUD_INFERENCE_API_URL=https://your-cloud-function-url.com/api/detect

# Feature Flags
ENABLE_CLOUD_BACKUP=true
ENABLE_OFFLINE_MODE=true
ENABLE_NOTIFICATIONS=true
ENABLE_ML_TELEMETRY=false

# Reminder Configuration
DEFAULT_REMINDER_DAYS=3
MAX_REMINDER_DAYS=7

# Notes:
# 1. Copy this file to .env and fill in your actual values
# 2. Never commit .env to version control
# 3. Place google-services.json in android/app/
# 4. Place GoogleService-Info.plist in ios/Runner/
